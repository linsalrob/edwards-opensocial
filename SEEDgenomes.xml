<?xml version="1.0" encoding="UTF-8"?>
<Module>
    <ModulePrefs
            title="Search SEED Genomes"
            author="Rob Edwards"
            author_email="raedwards@gmail.com"
            description="A demonstration SEED Web services Open Social">
        <Require feature="opensocial-0.8" />
        <Require feature="views" />
        <Require feature="dynamic-height" />
        <Require feature="tabs" />
    </ModulePrefs>
    <Content type="html" view="home,profile,canvas"><![CDATA[<!-- Get the genomes and create a list -->
<div>
   <div>
     <span id='friendViewer'></span>
   </div>
   		
   <div>
        <ul id='friends'></ul>
   </div>
   <div>
   <div>
        <ul id='friendsSearches'></ul>
   </div>
   <div>
    <span id='genomes'></span>
  </div>
	<button onclick='showSearch();'>Choose a genome</button>
  <div>
  	<span id='searchBox'></span>
  </div>
  <div>
  	<span id='results'></span>
  </div>
  <div>
  	<span id='errors'></span>
  </div>
  <div>
  	<span id='caughtErrors'></span>
  </div>
 
</div>

<script type="text/javascript" src="http://edwards.sdsu.edu/OpenSocialApps/SEEDgenomes.js"></script>

<script type="text/javascript">
	gadgets.util.registerOnLoadHandler(init());
</script>]]></Content>
</Module>
